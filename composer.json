{
    "name": "apiato/social-auth-container",
    "description": "An Apiato container which adds social authentication functionality using Laravel Socialite",
    "type": "apiato-container",
    "require": {
        "laravel/socialite": "^5.2.3",
        "apiato/core": "dev-master"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "orchestra/testbench": "^6.21"
    },
    "license": "MIT",
    "extra": {
        "apiato": {
            "container": {
                "name": "SocialAuth"
            }
        },
        "laravel": {
            "providers": [
                "App\\Containers\\SocialAuth\\Providers\\SocialAuthServiceProvider"
            ]
        }
    }
}
